<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.xapxinh.forex.server.entity.candle.EurUsdM5Candle" table="eur_usd_m5_candles">
		
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		
		<property name="name">
			<column name="name" />
		</property>
		
		<property name="time">
			<column name="time" sql-type="datetime"/>
		</property>		
			
		<property name="high">
			<column name="high" />
		</property>
		
		<property name="close">
			<column name="close" />
		</property>
		
		<property name="low">
			<column name="low" />
		</property>
		
		<property name="open">
			<column name="open" />
		</property>
		
		<property name="volume">
			<column name="volume" />
		</property>
        
	</class>
	
</hibernate-mapping>