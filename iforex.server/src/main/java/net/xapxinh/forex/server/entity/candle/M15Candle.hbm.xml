<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.xapxinh.forex.server.entity.candle.M15Candle" table="m15_candles">
		
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
        
        <property name="time">
			<column name="time" sql-type="datetime"/>
		</property>
				
		<property name="high">
			<column name="high" />
		</property>
		
		<property name="close">
			<column name="close" />
		</property>
		
		<property name="low">
			<column name="low" />
		</property>
		
		<property name="open">
			<column name="open" />
		</property>
		
		<property name="volume">
			<column name="volume" />
		</property>
		
		<property name="volbuy">
			<column name="volbuy" />
		</property>
		
		<bag name="waves" table="m15_waves_candles"
        	inverse="false" lazy="true" fetch="select" order-by="wave_id">
            <key>
                <column name="candle_id" not-null="true" />
            </key>
            <many-to-many entity-name="net.xapxinh.forex.server.entity.wave.M15Wave">
                <column name="wave_id" not-null="true" />
            </many-to-many>
		</bag>
	</class>
	
</hibernate-mapping>