<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.xapxinh.forex.server.entity.wave.EurUsdH4Wave" table="eur_usd_h4_waves">
		
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		
		<property name="name">
			<column name="name" />
		</property>
		
        <bag name="waveCandles" table="eur_usd_h4_waves_candles" inverse="true" lazy="true"
			fetch="select" order-by="id">
			<key column="wave_id" not-null="true">
			</key>
			<one-to-many entity-name="net.xapxinh.forex.server.entity.wave.EurUsdH4WaveCandle" />
		</bag>
		
	</class>
	
</hibernate-mapping>