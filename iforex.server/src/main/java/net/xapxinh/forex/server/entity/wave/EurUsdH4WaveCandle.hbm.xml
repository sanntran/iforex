<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.xapxinh.forex.server.entity.wave.EurUsdH4WaveCandle" table="eur_usd_h4_waves_candles">
		
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		
        <many-to-one name="wave" class="net.xapxinh.forex.server.entity.wave.EurUsdH4Wave" fetch="select">
            <column name="wave_id" not-null="false" />
        </many-to-one>
        
        <many-to-one name="candle" class="net.xapxinh.forex.server.entity.candle.EurUsdH4Candle" fetch="select">
            <column name="candle_id" not-null="false" />
        </many-to-one>
		
	</class>
	
</hibernate-mapping>