<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.xapxinh.forex.server.entity.wave.M15Wave" table="m15_waves">
		
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		
		<property name="name">
			<column name="name" />
		</property>
		
		<bag name="candles" table="m15_waves_candles"
        	inverse="false" lazy="true" fetch="select" order-by="candle_id">
            <key>
                <column name="wave_id" not-null="true" />
            </key>
            <many-to-many entity-name="net.xapxinh.forex.server.entity.candle.M15Candle">
                <column name="candle_id" not-null="true" />
            </many-to-many>
		</bag>
	</class>
	
</hibernate-mapping>